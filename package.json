{
  "name": "mhtml2html",
  "version": "3.0.0",
  "description": "Converts an mhtml document to a single html document",
  "author": "Mayank Sindwani",
  "license": "MIT",
  "homepage": "https://github.com/puntowav/mhtml2html#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puntowav/mhtml2html.git"
  },
  "files": [
    "dist/",
    "index.js",
    "LICENSE.txt",
    "README.md"
  ],
  "main": "dist/mhtml2html.js",
  "bin": {
    "mhtml2html": "./index.js"
  },
  "scripts": {
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\" \"*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\" \"*.js\"",
    "prebuild": "npm run lint",
    "build": "webpack --mode=production",
    "pretest": "npm run build",
    "test": "mocha tests/ && karma start --single-run",
    "compare": "node screenshot-compare.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "jsdom": "^24.0.0",
    "quoted-printable": "^1.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "babel-loader": "^9.1.3",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "karma": "^6.4.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^10.3.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.2.5",
    "puppeteer": "^22.0.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
