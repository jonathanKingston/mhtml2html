@charset "utf-8";

:root { --bg: #0c0a09; --surface: #1c1917; --border: #292524; --text: #fafa=
f9; --muted: #a8a29e; --emerald: #10b981; --cyan: #06b6d4; }

* { margin: 0px; padding: 0px; box-sizing: border-box; }

body { font-family: "Segoe UI", system-ui, sans-serif; background: var(--bg=
); color: var(--text); min-height: 100vh; padding: 3rem; }

h1 { font-size: 2rem; margin-bottom: 0.5rem; }

.subtitle { color: var(--muted); margin-bottom: 3rem; }

.test-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(=
300px, 1fr)); gap: 2rem; }

.test-section { background: var(--surface); border: 1px solid var(--border)=
; border-radius: 1rem; padding: 1.5rem; }

.test-section h2 { font-size: 0.875rem; text-transform: uppercase; letter-s=
pacing: 0.1em; color: var(--emerald); margin-bottom: 1rem; display: flex; a=
lign-items: center; gap: 0.5rem; }

.test-section h2::before { content: ""; width: 8px; height: 8px; background=
: var(--emerald); border-radius: 50%; }

.image-container { background: var(--bg); border-radius: 0.5rem; padding: 1=
rem; display: flex; align-items: center; justify-content: center; min-heigh=
t: 150px; }

.image-container img { max-width: 100%; height: auto; border-radius: 0.25re=
m; }

.inline-svg { display: flex; gap: 1rem; flex-wrap: wrap; justify-content: c=
enter; }

.inline-svg svg { width: 80px; height: 80px; }

.data-uri-test img { width: 100%; max-width: 200px; }

.bg-image-test { height: 150px; border-radius: 0.5rem; background-size: cov=
er; background-position: center center; display: flex; align-items: flex-en=
d; padding: 1rem; }

.bg-image-test span { background: rgba(0, 0, 0, 0.7); padding: 0.25rem 0.75=
rem; border-radius: 0.25rem; font-size: 0.75rem; }

.status { margin-top: 1rem; font-size: 0.75rem; color: var(--muted); }
