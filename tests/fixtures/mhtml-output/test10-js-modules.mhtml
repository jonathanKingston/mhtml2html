@charset "utf-8";

:root { --bg: #09090b; --surface: #18181b; --border: #27272a; --text: #fafa=
fa; --muted: #71717a; --accent: #a855f7; --success: #22c55e; --danger: #ef4=
444; }

* { margin: 0px; padding: 0px; box-sizing: border-box; }

body { font-family: system-ui, sans-serif; background: var(--bg); color: va=
r(--text); min-height: 100vh; padding: 3rem; }

.container { max-width: 900px; margin: 0px auto; }

h1 { font-size: 2rem; margin-bottom: 0.5rem; }

