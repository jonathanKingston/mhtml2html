            <div class=3D"flip-card-back">BACK</div>
          </div>
        </div>
      </div>
    </div>
   =20
    <!-- 10. Particles -->
    <div class=3D"demo-card">
      <h2>Floating Particles</h2>
      <div class=3D"demo-area">
        <div class=3D"particles">
          <div class=3D"particle"></div>
          <div class=3D"particle"></div>
          <div class=3D"particle"></div>
          <div class=3D"particle"></div>
          <div class=3D"particle"></div>
        </div>
      </div>
    </div>
   =20
    <!-- 11. Stagger -->
    <div class=3D"demo-card">
      <h2>Staggered Delay</h2>
      <div class=3D"demo-area">
        <div class=3D"stagger-container">
          <div class=3D"stagger-item"></div>
          <div class=3D"stagger-item"></div>
          <div class=3D"stagger-item"></div>
          <div class=3D"stagger-item"></div>
          <div class=3D"stagger-item"></div>
        </div>
      </div>
    </div>
   =20
    <!-- 12. Progress Ring -->
    <div class=3D"demo-card">
      <h2>SVG Animation</h2>
      <div class=3D"demo-area">
        <svg class=3D"progress-ring" viewBox=3D"0 0 80 80">
          <defs>
            <linearGradient id=3D"progressGradient" x1=3D"0%" y1=3D"0%" x2=
=3D"100%" y2=3D"100%">
              <stop offset=3D"0%" style=3D"stop-color:#22d3ee"></stop>
              <stop offset=3D"100%" style=3D"stop-color:#a855f7"></stop>
            </linearGradient>
          </defs>
          <circle class=3D"bg" cx=3D"40" cy=3D"40" r=3D"36"></circle>
          <circle class=3D"progress" cx=3D"40" cy=3D"40" r=3D"36"></circle>
        </svg>
      </div>
    </div>
  </div>


</body></html>
